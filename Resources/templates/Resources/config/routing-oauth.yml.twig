keboola_{{ appName }}_extractor_oauth:
    pattern:  /oauth
    defaults: { _controller: Keboola{{ classPrefix }}ExtractorBundle:OAuth:getOAuth }
    requirements:
        _method:  POST

keboola_{{ appName }}_extractor_oauth_external:
    pattern:  /oauth
    defaults: { _controller: Keboola{{ classPrefix }}ExtractorBundle:OAuth:externalAuth }
    requirements:
        _method:  GET

keboola_{{ appName }}_extractor_oauth_callback:
    pattern:  /oauth-callback
    defaults: { _controller: Keboola{{ classPrefix }}ExtractorBundle:OAuth:getOAuthCallback }
